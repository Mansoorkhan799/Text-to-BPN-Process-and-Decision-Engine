{
  "version": 2,
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "regions": ["iad1"],
  "installCommand": "npm install --platform=linux --arch=x64",
  "buildCommand": "npm run build",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "OTEL_SDK_DISABLED": "true"
  }
}
